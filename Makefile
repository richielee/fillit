NAME = fillit
FLAG = -Wall -Werror -Wextra
SRC = tetriminos_reader.c main.c
OBJ = $(SRC:.c=.o)

all: $(NAME)

$(NAME) :
	gcc -c $(FLAG) $(SRC) -I .
